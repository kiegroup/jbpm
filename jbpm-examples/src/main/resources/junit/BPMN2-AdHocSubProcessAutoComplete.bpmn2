      <bpmndi:BPMNShape bpmnElement="_4" >
        <dc:Bounds x="424" y="65" width="48" height="48" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge bpmnElement="_1-_2" >
        <di:waypoint x="40" y="91" />
