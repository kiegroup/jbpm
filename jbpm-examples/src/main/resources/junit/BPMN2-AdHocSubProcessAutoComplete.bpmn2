             xmlns:tns="http://www.jboss.org/drools">

  <process processType="Private" isExecutable="true" id="AdHocSubProcess" name="AdHoc SubProcess" >

    <!-- nodes -->
    <startEvent id="_1" name="StartProcess" />
    <adHocSubProcess id="_2" name="Hello"  ordering="Parallel" >
    <!-- nodes -->
    <userTask id="_2-1" name="Hello1" >
      <ioSpecification>
        <inputSet>
        </inputSet>
        <outputSet>
        </outputSet>
      </ioSpecification>
    </userTask>
    <!-- connections -->
    <completionCondition xsi:type="tFormalExpression">getActivityInstanceAttribute("numberOfActiveInstances") == 0</completionCondition>
    </adHocSubProcess>
    <scriptTask id="_3" name="Goodbye" >
      <script>System.out.println("Goodbye World");</script>
    </scriptTask>
    <endEvent id="_4" name="EndProcess" >
        <terminateEventDefinition/>
    </endEvent>

    <!-- connections -->
    <sequenceFlow id="_1-_2" sourceRef="_1" targetRef="_2" />
    <sequenceFlow id="_2-_3" sourceRef="_2" targetRef="_3" />
    <sequenceFlow id="_3-_4" sourceRef="_3" targetRef="_4" />

  </process>

  <bpmndi:BPMNDiagram>
